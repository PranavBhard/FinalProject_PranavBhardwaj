void hideSprites()
{
    int i;
    for(i = 0; i < 128; i++)
    {
         shadowOAM[i].attr0 = (2 << 8);
    }
}

void animate()
{

        if (gotKey) {
            key.currFrame = 16;
        }

        if (player.aniState != PLAYERIDLE)
        {
            player.prevAniState = player.aniState;
        }

        player.aniState = PLAYERIDLE;

        if(player.aniCounter%20==0)
        {
            player.aniCounter = 0;
            if (player.currFrame)
            {
                player.currFrame = 0;
            }
            else
            {
                player.currFrame++;
            }

        }

        if((~(*(volatile unsigned int *)0x04000130) & ((1<<6))))
        {
            player.aniState = PLAYERBACK;
        }
        if((~(*(volatile unsigned int *)0x04000130) & ((1<<7))))
        {
            player.aniState = PLAYERFRONT;
        }
        if((~(*(volatile unsigned int *)0x04000130) & ((1<<5))))
        {
            player.aniState = PLAYERLEFT;
        }
        if((~(*(volatile unsigned int *)0x04000130) & ((1<<4))))
        {
            player.aniState = PLAYERRIGHT;
        }

        if(player.aniState == PLAYERIDLE)
        {
            player.currFrame = 0;
        }
        else
        {
            player.aniCounter++;
        }



        for (int i=0; i<7; i++) {
            fireballs[i].prevAniState = fireballs[i].aniState;
            if (fireballs[i].rdel < 0 && fireballs[i].cdel > 0) {
                fireballs[i].aniState = UPRIGHT;
            } else if (fireballs[i].rdel > 0 && fireballs[i].cdel > 0) {
                fireballs[i].aniState = DOWNRIGHT;
            } else if (fireballs[i].rdel < 0 && fireballs[i].cdel < 0) {
                fireballs[i].aniState = UPLEFT;
            } else {
                fireballs[i].aniState = DOWNLEFT;
            }
        }

}
void updateOAM()
{
    shadowOAM[0].attr0 = (0xFF & player.row) | (0 << 13) | (0 << 14);
    shadowOAM[0].attr1 = (0x1FF & player.col) | (2 << 14);
    shadowOAM[0].attr2 = (4*player.currFrame)*32+(4*player.prevAniState);

    shadowOAM[1].attr0 = (0xFF & key.row) | (0 << 13) | (0 << 14);
    shadowOAM[1].attr1 = (0x1FF & key.col) | (2 << 14);
    shadowOAM[1].attr2 = (key.currFrame)*32+(0);


    for (int i=0; i<7; i++) {
        shadowOAM[i+2].attr0 = (0xFF & fireballs[i].row) | (0 << 13) | (0 << 14);
        shadowOAM[i+2].attr1 = (0x1FF & fireballs[i].col) | (2 << 14);
        shadowOAM[i+2].attr2 = (fireballs[i].currFrame*4)*32+((fireballs[i].prevAniState + 4)*4);
    }



}


