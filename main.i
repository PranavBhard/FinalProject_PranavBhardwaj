                }
            } else {
                playSoundB(falling, 56908, 11025);
                soundB.loops = 0;
                state = 2;
                dma[1].cnt = 0;
                *(volatile unsigned short*)0x4000102 = 0;
                soundA.isPlaying = 0;
            }

        }

        if((~(*(volatile unsigned int *)0x04000130) & ((1<<5))))
        {
            if (collMapBitmap[(((player.bigRow+(player.width/2))*(512)+(player.bigCol+(player.width/2))))/2] != 0x0303) {
                if (player.col > 120) {
                    player.col--;
                } else {
                    if (player.col == player.bigCol) {
                        if (player.col > 0) {
                            player.col--;
                        }
                    } else {
